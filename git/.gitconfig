[user]
  name = Priscila Bey
	email = pri.bey@gmail.com
[diff]
	noprefix = true
[color]
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
	meta = yellow
	frag = magenta bold
	old = red bold
	commit = yellow bold
	new = green
	whitespace = red reverse
[color "status"]
  added = green
  changed = yellow
  untracked = cyan
[alias]
  st = status -sb
  ci = commit --verbose
  co = checkout
  d = diff --compaction-heuristic
  dc = diff --cached
  amend = commit --amend
  aa = add --all
  ff = merge --ff-only
  pullff = pull --ff-only
  noff = merge --no-ff
  pom = push origin master
  cam = commit -am
  pr = pull --rebase
  head = !git l -1
  h = !git head
  r = !git l -20
  ra = !git r --all
  br = branch
  co = checkout
  up = pull --rebase --autostash

  r = !git l -30
  ra = !git r --all
  la = !git l --all
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  div = divergence
  gn = goodness
  gnc = goodness --cached
  fa = fetch --all
  undo = "!gitundo() { \
        git reset --hard HEAD\"@{${1-1}}\"; \
    }; gitundo"
[format]
  pretty=format:%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset
[core]
	pager = diff-so-fancy | less --tabs=4 -RFX
[i18n]
	logOutputEncoding = UTF-8
	commitencoding = UTF-8
[github]
	user = puribey
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
